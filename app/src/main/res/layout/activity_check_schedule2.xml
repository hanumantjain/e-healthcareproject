<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".checkSchedule2">


    <Button
        android:id="@+id/button_fill_case_history"
        android:layout_width="250dp"
        android:layout_height="60dp"
        android:text="FILL CASE HISTORY"
        android:textAllCaps="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.465"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.095"
        android:background="@drawable/custom_button"
        android:textColor="#FFFFFF"/>

    <Button
        android:id="@+id/button_upload_prescription"
        android:layout_width="250dp"
        android:layout_height="60dp"
        android:layout_marginTop="60dp"
        android:text="Upload Prescription"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.465"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_fill_case_history"
        android:background="@drawable/custom_button"
        android:textColor="#FFFFFF"/>

    <Button
        android:id="@+id/button_upload_lab_reports"
        android:layout_width="250dp"
        android:layout_height="60dp"
        android:layout_marginTop="60dp"
        android:text="UPLOAD LAB REPORTS"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.465"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_upload_prescription"
        android:background="@drawable/custom_button"
        android:textColor="#FFFFFF"/>

    <Button
        android:id="@+id/button_provide_transaction"
        android:layout_width="250dp"
        android:layout_height="60dp"
        android:layout_marginTop="60dp"
        android:text="PROVIDE TRANSACTIONS"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.465"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_upload_lab_reports"
        android:background="@drawable/custom_button"
        android:textColor="#FFFFFF"/>
</androidx.constraintlayout.widget.ConstraintLayout>